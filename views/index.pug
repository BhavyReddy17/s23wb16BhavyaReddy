extends layout

block content
  h1= title
  p Welcome Bhavya Reddy Sanku
  button(type='button' class="btn btn-secondary" onclick="doReduce()") Press Me
  div(id="reduce") Reduce the value
  script.
    var count = 20;
    var change = -1;
    function doReduce() {
      count += change;
      change -= 1;
      document.getElementById("reduce").textContent = `Count ${count} with change ${change}`;
    }